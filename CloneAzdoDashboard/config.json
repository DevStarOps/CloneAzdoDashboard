{
  "SourceTeamProjectBaseUri": "https://dev.azure.com/dah-dah/Default",
  "TargetTeamProjectBaseUri": "https://dev.azure.com/dah-dah/Project Awesome",
  "SourcePatKey": "",
  "TargetPatKey": "",

  "Mode": "MoveQueries", // Queries, MoveQueries or Dashboard

  "SourceTeamName": "Default Team",
  "SourceDashboardName": "Focus",

  "TargetTeamName": "Project Awesome Team",
  "TargetDashboardName": "Focus",

  "UpdateQueriesOnly": false,
  "DeleteTargetDashboardIfExists": true,
  "NullSettingsWhereNoSupportedProcessorExists": true,

  "Queries": {
    "PathFind": "My Folder",
    "PathReplace": "Your Folder",

    "QueryFindAndReplace": [
      {
        "Find": "[System.WorkItemType] <> ''",
        "Replace": "[System.WorkItemType] = 'Epic'"
      }
    ]
  },

  "Repos": {
    "Mapping": {
      "SampleApp": "Sample-App"
    }
  },

  "Builds": {
    "Mapping": {
      "SampleApp (1)": "SampleApp"
    }
  },

  "MarkdownFindAndReplace": [
    {
      "Find": null,
      "Replace": null
    }
  ]
}
